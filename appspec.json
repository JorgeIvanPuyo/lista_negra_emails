{
  "version": 1,
  "Resources": [
    {
      "TargetService": {
        "Type": "AWS::ECS::Service",
        "Properties": {
          "TaskDefinition": "<TASK_DEFINITION>",
          "LoadBalancerInfo": {
            "ContainerName": "Container-black-list-emails",
            "ContainerPort": 3000
          }
        }
      }
    }
  ],
  "TrafficRoutingConfig": {
    "Type": "TimeBasedCanary",
    "TimeBasedCanary": {
      "CanaryPercentage": 10,
      "CanaryInterval": 1
    }
  }
}
